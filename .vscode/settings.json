{
  "files.associations": {
    "dockerfile.*": "dockerfile"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/node_modules":true,
    "**/.ipynb_checkpoints":true,
    "**/*.pyc":true,
    "**/__pycache__":true
},
"python.pythonPath": "${workspaceRoot}/.env/bin/python"
}